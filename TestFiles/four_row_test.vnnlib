; 4-input vnnlib: only input constraints

(declare-const X_0 Real)
(declare-const X_1 Real)
(declare-const X_2 Real)
(declare-const X_3 Real)

; Input box:
; X_0 in [-1.0, 1.0]
; X_1 in [ 0.0, 2.0]
; X_2 in [ 0.5, 0.5]  (fixed value)
; X_3 in [-3.0,-2.0]
(assert (and
  (>= X_0 -1.0) (<= X_0  1.0)
  (>= X_1  0.0) (<= X_1  2.0)
  (>= X_2  0.5) (<= X_2  0.5)
  (>= X_3 -3.0) (<= X_3 -2.0)
))
